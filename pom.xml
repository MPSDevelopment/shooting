<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>tech.shooting</groupId>
	<artifactId>parent</artifactId>
	<version>0.1-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>parent</name>

	<profiles>
		<profile>
			<id>ipsc</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<modules>
				<module>shooting-commons</module>
				<module>shooting-ipsc</module>
			</modules>
		</profile>
		<profile>
			<id>docker</id>
			<modules>
				<module>shooting-commons</module>
				<module>shooting-ipsc</module>
			</modules>
		</profile>

	</profiles>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-deploy-plugin</artifactId>
				<version>${deploy.plugin.version}</version>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<properties>

		<docker.image.name>ipsc</docker.image.name>
		<docker.image.version>0.2</docker.image.version>
		<jdk.version>11</jdk.version>

		<lib.property>lib</lib.property>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>

		<resources.plugin.version>3.1.0</resources.plugin.version>
		<compiler.plugin.version>3.8.0</compiler.plugin.version>
		<surefire.plugin.version>2.22.1</surefire.plugin.version>
		<dependency.plugin.version>3.1.1</dependency.plugin.version>
		<jar.plugin.version>3.1.0</jar.plugin.version>
		<deploy.plugin.version>2.8.2</deploy.plugin.version>
		<minify.plugin.version>1.7.4</minify.plugin.version>
		<antrun.plugin.version>1.8</antrun.plugin.version>
		<exec.maven.plugin.version>1.4.0</exec.maven.plugin.version>
		<assembly.plugin.version>3.0.0</assembly.plugin.version>
		<source.plugin.version>2.4</source.plugin.version>
		<javadoc.plugin.version>2.10.4</javadoc.plugin.version>
		<exec.maven.plugin.version>1.6.0</exec.maven.plugin.version>
		<dockerfile.plugin.version>1.4.10</dockerfile.plugin.version>

		<plasticine.version>0.9</plasticine.version>
		<logback.version>1.2.3</logback.version>
		<logstash.version>4.11</logstash.version>
		<hikari.version>2.6.0</hikari.version>
