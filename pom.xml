		<spring.data.version>2.1.9.RELEASE</spring.data.version>
		<spring.amqp.version>2.1.7.RELEASE</spring.amqp.version>
		<moquette.version>0.12.1</moquette.version>
		<servlet.version>3.1.0</servlet.version>
		<junit.version>5.4.0</junit.version>
		<mockito.version>2.24.0</mockito.version>
		<wiremock.version>2.21.0</wiremock.version>
		<mbassador.version>1.3.2</mbassador.version>
